repos:
  - name: my-project
    path: ../my-project
    test_command: "cargo test"
    base_branch: main
    extra_tools: []

settings:
  parallel_workers: 4
  models:
    triage: haiku
    default: sonnet
    complex: opus
  worker_tools:
    - Bash
    - Read
    - Write
    - Edit
    - Glob
    - Grep
  poll_interval_secs: 300
  worktree_dir: .pfl-worktrees
  state_file: ~/.pfl-forge/state.yaml
